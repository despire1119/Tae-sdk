<!-- checkbox样式开始 -->
<c:forEach items="${parameter.optionsList}" var="option">
	<div class="rc-box">
		<label><input type="checkbox" name="${parameter.name}"
			value="${option.value}"
			<c:if test="${not empty option.selected}">
					checked="checked"
					</c:if> />
			${option.content}</label>
	</div>
</c:forEach>
<!-- checkbox样式结束 -->







