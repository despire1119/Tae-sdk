                                  log.error('rule-2.xml,permission8801,nick:' + nick)
                                  result.setSuccess(true)
                                  return result;
                                }
                                def extraUserDO = user?.extraUserDO

                                def hasTag = false
                                if (extraUserDO) {
                                    def tag7 = extraUserDO.userTag7
