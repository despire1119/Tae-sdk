            <permission permId="116"
                        scope="edit" desc="检查用户是否有实体店页面的使用权限">
                <scriptcode><![CDATA[import com.taobao.tae.design.rules.validate.ValidateResult
                  def execute(context) {
                        def result = new ValidateResult()
                        def isLifeUser = false
	                    try{
	                           def user = context.userDO
                               def rs = uicDataServiceClient.getData(user.userId,'bendi-lifeplace-userdata')
		                       if(rs?.isSuccess()){
		                          def model = rs.getModule()
		                          if(model){
		                              isLifeUser = (model.toLong() & 2) ==2
		                          }
		                          log.error('isLifeUser '+isLifeUser)
		                       }
	                    }catch(e){
	                        result.setSuccess(false)
	                        result.errorMessage = '系统出错'
	                        log.error('validate permission 116 in rule 2 errors',e)
	                    }
	                    
	                    if(!isLifeUser){
	                        result.errorMessage = '没有实体证页面权限'
	                        result.setSuccess(false)
	                    }
                        result
                }
                ]]></scriptcode>
            </permission>
            <permission permId="4007" scope="edit" desc="检查是否是超级用户">
                <scriptcode>
                     <![CDATA[import com.taobao.tae.design.rules.validate.ValidateResult
                                                         import com.taobao.shopservice.core.client.util.ShopFeatureConstant
                                                         import groovy.json.JsonSlurper
                                 def execute(context) {
                                		def result = new ValidateResult();
                                		def success = false
                                		try{
                                		  def shop = context.shopDO
                                		  def isFull = shop?.getFeature(ShopFeatureConstant.IS_FULL_SELF_USER)
                                		  if(isFull == 'true'){
                                		     success = true
                                		  }else{
                                		        def map = parseJsonToMap(isFull)
                                                if(map.fullSelf == 'true'){
                                                    success = true
                                                }
                                		  }
                                		}catch(e){
                                		  log.error('validate is Full self user errors',e)
                                		}
                                		result.setSuccess(success)
                                		return result
                                	}
                 def parseJsonToMap(jsonStr){
                    def paramsMap = [:]
                    if(!jsonStr)return paramsMap
                    JsonSlurper json = new JsonSlurper()
                    try{
                        paramsMap = json.parseText(jsonStr)
                    }catch(e){
                        return paramsMap
                    }
                    paramsMap
                }]]>
                </scriptcode>
            </permission>
